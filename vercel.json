{
    "version": 2,
    "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://13.124.151.164:8080/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "http://13.124.151.164:8080/:path*/"
    }
  ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/firebase-messaging-sw.js",
            "dest": "/firebase-messaging-sw.js"
          },

        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": "/images/(.*)",
            "dest": "/images/$1"
         },
        {
            "src": "/logo192.png",
            "dest": "/logo192.png"
        },
        {
            "src": "/logo512.png",
            "dest": "/logo512.png"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
